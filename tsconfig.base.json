{
  "compilerOptions": {
    "declaration": true,
    "target": "esnext",
    "module": "NodeNext",
    "moduleResolution": "nodenext",
    "sourceMap": true,
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "strictNullChecks": true,
    "skipLibCheck": true
  },
}
