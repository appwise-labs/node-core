{
  "name": "@wisemen/vue-cli",
  "version": "0.1.0",
  "description": "Add components to your apps.",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "author": {
    "name": "Robbe Vaes"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "components",
    "ui",
    "tailwind",
    "vue",
    "shadcn"
  ],
  "type": "module",
  "exports": "./dist/index.js",
  "bin": "./dist/index.js",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist && rimraf components",
    "start:dev": "cross-env COMPONENTS_BASE_URL=http://localhost:3001 node dist/index.js",
    "start": "node dist/index.js",
    "changeset": "changeset",
    "changeset-version": "changeset version",
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "pub:beta": "pnpm publish --no-git-checks --access public --tag beta",
    "pub:next": "npm publish --no-git-checks --access public --tag next",
    "pub:release": "pnpm publish --access public",
    "prebuild": "rm -rf ./dist",
    "pretest": "pnpm run clean && pnpm typecheck && pnpm run build",
    "prerelease": "pnpm run build",
    "release": "pnpm pretest && release-it"
  },
  "dependencies": {
    "@appwise/forms": "^0.0.19",
    "chalk": "5.2.0",
    "class-variance-authority": "^0.6.1",
    "commander": "^10.0.0",
    "cosmiconfig": "^8.1.3",
    "diff": "^5.1.0",
    "eslint": "^8.47.0",
    "execa": "^7.0.0",
    "find-up": "^6.3.0",
    "fs-extra": "^11.1.0",
    "https-proxy-agent": "^6.2.0",
    "node-fetch": "^3.3.0",
    "ora": "^6.1.2",
    "prompts": "^2.4.2",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.3.3",
    "tsconfig-paths": "^4.2.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@types/diff": "^5.0.3",
    "@types/fs-extra": "^11.0.1",
    "@types/prompts": "^2.4.2",
    "changeset": "^0.2.6",
    "release-it": "^16.1.5",
    "rimraf": "^4.1.3",
    "tsup": "^6.6.3",
    "type-fest": "^3.8.0",
    "typescript": "^4.9.3"
  }
}